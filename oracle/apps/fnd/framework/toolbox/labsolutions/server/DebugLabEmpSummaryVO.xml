      </DesignTime>
   </EntityUsage>
   <ViewAttribute
      Name="EmployeeId"
      IsUpdateable="while_insert"
      IsNotNull="true"
      EntityAttrName="EmployeeId"
      EntityUsage="EmployeeEO"
      AliasName="EMPLOYEE_ID" >
   </ViewAttribute>
   <ViewAttribute
      Name="EmployeeName"
      EntityAttrName="FullName"
      EntityUsage="EmployeeEO"
      AliasName="EMPLOYEE_NAME" >
   </ViewAttribute>
   <ViewAttribute
