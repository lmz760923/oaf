                     <ui:contents>
                        <oa:formValue viewName="EmployeeFullVO1" viewAttr="ManagerId" id="MgrNum" dataType="NUMBER" prompt="Manager Number"/>
                     </ui:contents>
                  </oa:messageLayout>
               </ui:contents>
            </oa:messageComponentLayout>
         </ui:contents>
         <ui:pageStatus>
            <oa:flowLayout id="PageStatusRN">
               <ui:contents>
                  <oa:tableLayout id="RequiredKey" extends="/oracle/apps/fnd/framework/webui/OAReqFieldDescRG" hAlign="center" width="100%"/>
