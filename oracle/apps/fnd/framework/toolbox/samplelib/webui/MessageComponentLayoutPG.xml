<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.3_762" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header: MessageComponentLayoutPG.xml 120.7 2006/05/25 13:13:48 atgops1 noship $">
   <content>
      <oa:pageLayout id="PageLayoutRN" windowTitle="Message Component Layout" amDefName="oracle.apps.fnd.framework.toolbox.samplelib.server.SampleBrowserAM">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:header id="DoubleHdr" text="Standard Double Column">
               <ui:contents>
                  <oa:messageComponentLayout id="DoubleColumnRN" columns="2" rows="3">
                     <ui:contents>
                        <oa:messageStyledText id="EmpNum" dataType="NUMBER" prompt="Employee Number"/>
                        <oa:messageTextInput id="FirstName" prompt="First Name" columns="40"/>
                        <oa:messageTextInput id="LastName" prompt="Last Name" columns="50"/>
                        <oa:messageTextInput id="HireDate" dataType="DATE" text="Hire Date" prompt="Hire Date" tipType="dateFormat"/>
                        <oa:messageTextInput id="Salary" dataType="NUMBER" prompt="Salary" columns="15"/>
                     </ui:contents>
                  </oa:messageComponentLayout>
               </ui:contents>
            </oa:header>
         </ui:contents>
      </oa:pageLayout>
