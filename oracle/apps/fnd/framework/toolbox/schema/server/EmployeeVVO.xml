<ViewObject
   Name="EmployeeVVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.toolbox.schema.server.EmployeeVVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.toolbox.schema.server.EmployeeVVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   Passivate="None"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select employee_id
from fwk_tbx_employees
where sysdate between start_date and nvl(end_date, sysdate)
and employee_id = :1
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.43.6" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll" />
   </DesignTime>
   <ViewAttribute
      Name="EmployeeId"
      IsQueriable="false"
      IsPersistent="false"
      IsNotNull="true"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="EMPLOYEE_ID"
      Expression="EMPLOYEE_ID"
      SQLType="NUMERIC" >
   </ViewAttribute>
</ViewObject>
