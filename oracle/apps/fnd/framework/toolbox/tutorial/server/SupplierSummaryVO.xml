   </ViewAttribute>
   <ViewAttribute
      Name="OnHoldDisplay"
      IsQueriable="false"
      IsPersistent="false"
      IsNotNull="true"
      Precision="80"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="ON_HOLD_DISPLAY"
      Expression="ON_HOLD_DISPLAY"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="80" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="OnHoldFlag"
      EntityAttrName="OnHoldFlag"
      EntityUsage="SupplierEO"
      AliasName="ON_HOLD_FLAG" >
   </ViewAttribute>
</ViewObject>
